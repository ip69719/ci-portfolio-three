{
  "name": "ci-portfolio-three",
  "build": {
    "dockerfile": "../.gitpod.dockerfile"
  },
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "remoteUser": "gitpod",
  "containerUser": "gitpod",
  "workspaceFolder": "/workspace/ci-portfolio-three",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/ci-portfolio-three,type=bind",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "formulahendry.auto-close-tag",
        "eventyret.bootstrap-4-cdn-snippet",
        "hookyqr.beautify",
        "matt-rudge.auto-open-preview-panel"
      ]
    }
  }
}